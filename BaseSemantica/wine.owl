:Calice rdfs:comment "Il tipo di calice con cui va servito il vino"@it ;
        rdfs:label "Calice"@it ,
                   "Chalice"@en .


:Cantina rdfs:comment "Il luogo dove è stato prodotto il vino"@it ;
         rdfs:label "Cellar"@en ,
                    "Cantina"@it .


:annata rdfs:comment "indica l'annata del vino"@it ,
                     "indicates the vintage of the wine"@en ;
        rdfs:label "vintage"@en ,
                   "Annata"@it .


:gradazione rdfs:label "gradation"@en ;
