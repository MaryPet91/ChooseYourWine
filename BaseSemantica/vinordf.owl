    <!-- http://w3id.org/food/ontology/disciplinare-vino/vino -->

    <owl:Class rdf:about="http://w3id.org/food/ontology/disciplinare-vino/vino">
        <rdfs:subClassOf rdf:resource="http://w3id.org/food/ontology/disciplinare-upper/Prodotto"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#abbinato"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#cibo"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haAnnata"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#annata"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haDenominazione"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino/denominazioneVino"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haGradazione"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#gradazione"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haTemperatura"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#temperatura"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haTipologia"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/food/ontology/disciplinare-vino/tipologiaVino"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/food/ontology/disciplinare-vino#haVitigno"/>
                <owl:someValuesFrom rdf:resource="http://aims.fao.org/aos/agrovoc/vitigno"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Descrive l&apos;insieme di tutti i vini, che possono essere associati ad una certa denominazione (ad esempio, \&quot;Abruzzo\&quot;) e tipologia (ad esempio, \&quot;rosso\&quot;).</rdfs:comment>
        <rdfs:comment xml:lang="en">It represents all the wine products associated with a certain protected name and a certain type.</rdfs:comment>
        <rdfs:label xml:lang="it">vino</rdfs:label>
        <rdfs:label xml:lang="en">wine</rdfs:label>
    </owl:Class>
    


