                                                                         owl:onProperty :haGradazione ;
                                                                         owl:someValuesFrom :gradazione
                                                                       ] ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :haTemperatura ;
                                                                         owl:someValuesFrom :temperatura
                                                                       ] ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :haVitigno ;
                                                                         owl:someValuesFrom <http://aims.fao.org/aos/agrovoc/vitigno>
                                                                       ] ;
                                                       rdfs:comment "Descrive l'insieme di tutti i vini, che possono essere associati ad una certa denominazione (ad esempio, \\\"Abruzzo\\\") e tipologia (ad esempio, \\\"rosso\\\")."@it ,
                                                                    "It represents all the wine products associated with a certain protected name and a certain type."@en ;
                                                       rdfs:label "vino"@it ,
                                                                  "wine"@en .


